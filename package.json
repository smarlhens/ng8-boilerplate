{
  "name": "ng8-boilerplate",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "test": "ng test",
    "test:ci": "ng test -c=ci",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e2e:ci": "ng e2e -c=ci",
    "lint:scss": "stylelint 'src/**/*.scss' --syntax scss",
    "lint:scss:fix": "stylelint 'src/**/*.scss' --syntax scss --fix",
    "lint:js": "eslint . --ext .js,.jsx",
    "lint:js:fix": "npm run lint:js -- --fix",
    "prettier:ts:check": "prettier --check ./**/*.ts",
    "prettier:ts:write": "prettier --write ./**/*.ts",
    "prettier:js:check": "prettier --check ./**/*.js",
    "prettier:js:write": "prettier --write ./**/*.js",
    "prettier:scss:check": "prettier --check ./**/*.scss",
    "prettier:scss:write": "prettier --write ./**/*.scss",
    "prettier:json:check": "prettier --check ./**/*.{json,*rc}",
    "prettier:json:write": "prettier --write ./**/*.{json,*rc}",
    "prettier:check": "npm run prettier:ts:check && npm run prettier:js:check && npm run prettier:scss:check && npm run prettier:json:check",
    "prettier:write": "npm run prettier:ts:write && npm run prettier:js:write && npm run prettier:scss:write && npm run prettier:json:write",
    "precommit": "npm run prettier:write && npm run lint:js:fix && npm run lint && ng test --watch=false",
    "prepush": "ng test --watch=false",
    "postinstall": "webdriver-manager update --standalone false --gecko false"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "rxjs": "^6.4.0",
    "tslib": "^1.10.0",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.14",
    "@angular/cli": "^8.3.19",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@types/jasmine": "^3.3.8",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^8.9.4",
    "@typescript-eslint/eslint-plugin": "^2.5.0",
    "@typescript-eslint/parser": "^2.5.0",
    "codelyzer": "^5.0.0",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^6.5.1",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^3.0.9",
    "jasmine-core": "^3.4.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.1",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "lint-staged": "^9.4.3",
    "prettier": "^1.18.2",
    "protractor": "^5.4.2",
    "puppeteer": "^2.0.0",
    "stylelint": "^11.1.1",
    "stylelint-config-recommended": "^3.0.0",
    "ts-node": "^7.0.0",
    "tslint": "^5.15.0",
    "typescript": "^3.5.3"
  }
}
